
@using _00_Entity
@model List<Note>
<div id="favoriler-panel" class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">
            <a data-toggle="collapse" data-parent="#accordion" href="#collapsefive">
                <span class="glyphicon glyphicon-file">
                </span>Favoriler<span id="notebook-count" class="badge">@Model.Count</span>
            </a>
        </h4>
    </div>
    <div id="collapsefive" class="panel-collapse collapse">
        <div class="panel-body">
            <table class="table">
                <tbody>
                    @for (int i = 0; i < Model.Count; i++)
                    {

                        <tr>
                            <td>
                                <a href="~/Home/Note/@Model[i].ID">@Model[i].Title</a>
                            </td>
                        </tr>
                    }
                </tbody>
                <tbody id="favori-panel"></tbody>

                
            </table>
        </div>
    </div>
</div>

